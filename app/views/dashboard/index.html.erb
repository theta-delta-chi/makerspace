<div class="ui stackable grid">
  <div class="row">
    <h1 class="ui massive dividing header">
      <img src="http://tdc.mit.edu/summerhousing/images/crest.png" class='ui bottom aligned image'>
      <span>TDC Makerspace Emergency System</span></h1>
  </div>
  <div class="row">
    <%= render partial: 'contacts', locals: {contacts: @contacts} %>
    <div class="ten wide column">
      <div class='ui small header'>
        <%= @img_time %>
      </div>
      <div class='ui large centered images'>
        <img src="<%= @picam_link %>" class='ui image'>
        <img src="<%= @usbcam_link %>" class='ui image'>
      </div>
    </div>
  </div>
  <div id='contacts_form_container' class='row'>
    <%= render partial: 'contact_form', locals: {contacts: @contacts, new_contact: @new_contact} %>
  </div>
</div>

